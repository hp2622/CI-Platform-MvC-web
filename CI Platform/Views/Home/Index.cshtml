    <script>
      // Get the elements with class="column"
      var elements = document.getElementsByClassName("col");
      var imgs = document.getElementsByClassName("card-img-top");
      var card = document.getElementsByClassName("card");
      var interval = document.getElementsByClassName("missioninterval");
      var hr = document.getElementsByClassName("hr");
      //var badge = document.getElementsByClassName("notify-badge");
      //var smallscreen = document.getElementsByClassName("col-sm-12");
      // Declare a loop variable
      var i;

      // List View
      function listView() {
        for (i = 0; i < elements.length + 1; i++) {
          elements[i].style.width = "100%";
          imgs[i].style.width = "30%";
          card[i].style.display = "flex";
          card[i].style.flexDirection = "inherit";
          card[i].style.left = "0";
          interval[i].style.width = "65%";
          interval[i].style.left = "17%";
          hr[i].style.backgroundColor = "transparent";
          //   interval[i].style.top = "-21%";

          //   badge[i].style.right = "82.5%";
          //   badge[i].style.top = "87%";
          //   badge[i].style.width = "14%";
        }
      }

      // Grid View
      function gridView() {
        for (i = 0; i < elements.length; i++) {
          if (screen.width > 576 && screen.width < 992) {
            elements[i].style.width = "50%";
            interval[i].style.top = "-8%";
          } else if (screen.width < 576) {
            elements[i].style.width = "100%";
            interval[i].style.top = "-15%";
          } else {
            elements[i].style.width = "33%";
          }
          imgs[i].style.width = "100%";
          card[i].style.flexDirection = "column";
          hr[i].style.backgroundColor = "#c5c5c5";
          //   interval[i].style.top = "-9%";

          //   badge[i].style.right = "31%";
          //   badge[i].style.top = "43%";

          //   badge[i].style.width = "40%";
        }
      }

      /* Optional: Add active class to the current button (highlight it) */
      var container = document.getElementById("btnContainer");
      var btns = container.getElementsByClassName("btn");
      for (var i = 0; i < btns.length; i++) {
        btns[i].addEventListener("click", function () {
          var current = document.getElementsByClassName("active");
          current[0].className = current[0].className.replace(" active", "");
          this.className += " active";
        });
      }
    </script>

